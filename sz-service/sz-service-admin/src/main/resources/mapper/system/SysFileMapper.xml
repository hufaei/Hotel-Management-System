<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.admin.system.mapper.CommonFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sz.admin.system.pojo.po.SysFile">
        <id column="id" property="id"/>
        <result column="type" property="type"/>
        <result column="path" property="path"/>
        <result column="size" property="size"/>
        <result column="ext" property="ext"/>
        <result column="storage_method" property="storageMethod"/>
        <result column="url" property="url"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , type, path, size, ext, storage_method, url, create_time, update_time
    </sql>

</mapper>
